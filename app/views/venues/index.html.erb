<h1>Venues#index</h1>
<div class="container">
  <div class="row">
    <% @venues.each do |venue| %>
      <div class="col-md-4">
        <div class="card md-4" >
          <img src="<%= venue.img_url %>" class="card-img-top" alt="Listing Image">
          <div class="card-body">
            <h5 class="card-title"><%= venue.title %></h5>
            <p class="card-text">$<%= venue.daily_rate %></p>
            <%= link_to "details", venue_path(venue), class: "btn btn-outline-success" %>
          </div>
        </div>
      </div>
    <% end %>
  </div>
</div>
