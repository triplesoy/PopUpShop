<h1>My Bookings</h1>
<div class="container">
  <div class="row">
    <% @my_bookings.each do |booking| %>
      <div class="col-md-4">
        <div class="card md-4" >
          <img src="<%= booking.venue.img_url %>" class="card-img-top" alt="Listing Image">
          <div class="card-body">
            <h5 class="card-title"><%= booking.venue.title %></h5>
            <p class="card-text">$<%= booking.venue.daily_rate %></p>
            <%= link_to "details", venue_path(booking.venue), class: "btn btn-outline-success" %>
          </div>
        </div>
      </div>
    <% end %>
  </div>
</div>
